# stuff you might want to override
cpu=`dpkg-architecture -qDEB_HOST_GNU_CPU`
distribution=Debian

# common stuff
pwd=`pwd`
export GZIP=--best
distribution_lowcase=`echo ${distribution} | tr [A-Z] [a-z] | tr " " "_"`
version=`date +%Y%m%d`
cdname=${distribution_lowcase}-${version}-kfreebsd-${cpu}-install.iso
