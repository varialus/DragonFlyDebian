+     */
+    bb_error_msg("\n\nTo make this application work, you will need to recompile\n"
+	    "BusyBox with a kernel supporting the sysfs system call.\n");
+    errno=ENOSYS;
+    return -1;
+#else
+    return(syscall(__NR_sysfs, option, fs_index, buf));
+#endif
 }
 
