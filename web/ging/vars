distribution=ging
username=${distribution}
hostname=${distribution}

# the string that will be embedded in the livecd itself
version=0.1.0

# only used for the filename.  this way, RC versions can be converted into a
# final release without need to re-masterise.
rc=.rc1

cpu="i486"
system="kfreebsd-gnu"
uname="GNU/kFreeBSD"
unset GZIP
pwd=`pwd`

if [ "${OPTS}" = "qemu" ] ; then
  dot_gz=""
  gzip="true"
  version="${version}.qemu"
else
  dot_gz=".gz"
  gzip="gzip -9"
fi
