+	if (ioctl(0, GIO_SCRNMAP, &map) == -1) {
+		revert();
+		errc(1, errno, "getting screenmap");
+	}
+	for (i=0; i<sizeof(map); i++) {
+		if (i != 0 && i % 16 == 0)
+			fprintf(stdout, "\n");
+
+		if (hex != 0)
+			fprintf(stdout, " %02x", map[i]);
+		else
+			fprintf(stdout, " %03d", map[i]);
+	}
+	fprintf(stdout, "\n");
+
+}
