# Kernel for Debian GNU/kFreeBSD on an i486 machine

cpu		I486_CPU
cpu		I586_CPU
cpu		I686_CPU
makeoptions	COPTFLAGS="-Os -frename-registers -pipe -march=i486 -mtune=i486"
ident		Debian-i486

options		COMPAT_LINUX
options		LINPROCFS
options         LINSYSFS
options		FDESCFS
options		TMPFS

# UTF-8 console support
options		VESA
options		SC_PIXEL_MODE	# add support for the raster text mode
options		TEKEN_UTF8	# UTF-8 output handling

# Xbox support
options 	XBOX
device		xboxfb
device		fb


#include		GENERIC
