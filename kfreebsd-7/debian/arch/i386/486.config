# Kernel for Debian GNU/kFreeBSD on an i486 machine

cpu		I486_CPU
cpu		I586_CPU
cpu		I686_CPU
makeoptions	COPTFLAGS="-Os -frename-registers -pipe -march=i486 -mtune=i486"
ident		Debian-i486

options		COMPAT_LINUX
options		LINPROCFS
options         LINSYSFS
options		FDESCFS
options		TMPFS

# Xbox support
options 	XBOX
device		xboxfb
device		fb

# Use BOOTP to obtain IP address/hostname
options     BOOTP
options     BOOTP_COMPAT
# NFS mount root file system using BOOTP info
options     BOOTP_NFSROOT
options     BOOTP_NFSV3

#include		GENERIC
